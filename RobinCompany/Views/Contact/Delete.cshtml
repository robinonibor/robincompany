@model RobinCompany.Models.Contact
@using RobinCompany.Models

@using (Html.BeginForm("Delete", "Contact", FormMethod.Post, new { id = "popup" }))
{
    @Html.HiddenFor(x => x.ContactId);
    <div class="form-group">
        @{TitleofContact title = (TitleofContact)Model.ContactTitleId;
        }
        <label>Title :</label> @title
    </div>
    <div class="form-group">
        <label>First Name :</label> @Model.ContactFirstName
    </div>
    <div class="form-group">
        <label>Last Name :</label>
        @Model.ContactLastName
    </div>
    <div class="form-group">
        @{ContractType type = (ContractType)Model.ContactTitleId;
        }
        <label>Type of Contract :</label> @type
    </div>
    <div class="form-group">
        <label>Email :</label> @Model.Email
    </div>
    <div class="form-group">
        <label>PhoneNumber :</label> @Model.PhoneNumber
    </div>
    <div class="form-group">
        <label>Department :</label> @Model.Department
    </div>
    <div class="form-group">
        <label>IsActive :</label> @Model.ACTIVE
    </div>

    <div>
        <input type="submit" value="Delete" />
    </div>
}